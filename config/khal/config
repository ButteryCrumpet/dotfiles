[calendars]

[[private]]
path = ~/.calendars/private

[[work]]
path = ~/.calendars/work

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M

[default]
default_calendar = private
highlight_event_days = true
timedelta = 7 days
show_all_days = false

[highlight_days]
color = light green
