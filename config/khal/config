[calendars]

[[private]]
path = ~/.calendars/private
color = dark green

[[work]]
path = ~/.calendars/work
color = light magenta

[[holidays]]
path = ~/.calendars/holidays
readonly = True
color = light cyan

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M

[default]
default_calendar = private
highlight_event_days = true
timedelta = 7 days
show_all_days = false

[highlight_days]
default_color = dark green
